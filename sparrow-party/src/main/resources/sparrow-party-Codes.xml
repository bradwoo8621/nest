<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd
        http://www.springframework.org/schema/util 
    	http://www.springframework.org/schema/util/spring-util.xsd">


	<!-- code tables start here -->
	<!-- com.github.nest.sparrow.party.internal.codes.PartyType -->
	<bean
		id="PartyType"
		class="com.github.nest.arcteryx.meta.beans.internal.CachedBeanDescriptor"
		p:resourceClass="com.github.nest.sparrow.party.internal.codes.PartyType"
		p:name="PartyType"
		p:description="Party type"
		scope="singleton">
		<property name="properties">
			<list>
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="name"
					p:description="Name of party type" />
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="code"
					p:description="Code of party type" />
			</list>
		</property>
		<property name="operators">
			<list>
				<bean
					class="com.github.nest.sparrow.party.internal.operator.PartyTypeCacheProvider" />
			</list>
		</property>
	</bean>
	<!-- com.github.nest.sparrow.party.internal.codes.PartyRoleType -->
	<bean
		id="PartyRoleType"
		class="com.github.nest.arcteryx.meta.beans.internal.CachedBeanDescriptor"
		p:resourceClass="com.github.nest.sparrow.party.internal.codes.PartyRoleType"
		p:name="PartyRoleType"
		p:description="Party role type"
		scope="singleton">
		<property name="properties">
			<list>
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="name"
					p:description="Name of party role type" />
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="code"
					p:description="Code of party role type" />
			</list>
		</property>
		<property name="operators">
			<list>
				<bean
					class="com.github.nest.sparrow.party.internal.operator.PartyRoleTypeCacheProvider"
					init-method="readFromClassPath"
					p:fileClassPath="/codes/partyRoleTypes.list" />
			</list>
		</property>
	</bean>
	<!-- com.github.nest.sparrow.party.internal.codes.Industry -->
	<bean
		id="Industry"
		class="com.github.nest.arcteryx.meta.beans.internal.CachedBeanDescriptor"
		p:resourceClass="com.github.nest.sparrow.party.internal.codes.Industry"
		p:name="Industry"
		p:description="Industry"
		scope="singleton">
		<property name="properties">
			<list>
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="name"
					p:description="Name of industry" />
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="code"
					p:description="Code of industry" />
			</list>
		</property>
		<property name="operators">
			<list>
				<bean
					class="com.github.nest.sparrow.party.internal.operator.IndustryCacheProvider"
					init-method="readFromClassPath"
					p:fileClassPath="/codes/industry.list" />
			</list>
		</property>
	</bean>
	<!-- com.github.nest.sparrow.party.internal.codes.AcademicMajor -->
	<bean
		id="AcademicMajor"
		class="com.github.nest.arcteryx.meta.beans.internal.CachedBeanDescriptor"
		p:resourceClass="com.github.nest.sparrow.party.internal.codes.AcademicMajor"
		p:name="AcademicMajor"
		p:description="AcademicMajor"
		scope="singleton">
		<property name="properties">
			<list>
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="name"
					p:description="Name of academicMajor" />
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="code"
					p:description="Code of academicMajor" />
			</list>
		</property>
		<property name="operators">
			<list>
				<bean
					class="com.github.nest.sparrow.party.internal.operator.AcademicMajorCacheProvider"
					init-method="readFromClassPath"
					p:fileClassPath="/codes/academicMajors.list" />
			</list>
		</property>
	</bean>
	<!-- com.github.nest.sparrow.party.internal.codes.EducationDegree -->
	<bean
		id="EducationDegree"
		class="com.github.nest.arcteryx.meta.beans.internal.CachedBeanDescriptor"
		p:resourceClass="com.github.nest.sparrow.party.internal.codes.EducationDegree"
		p:name="EducationDegree"
		p:description="EducationDegree"
		scope="singleton">
		<property name="properties">
			<list>
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="name"
					p:description="Name of educationDegree" />
				<bean
					class="com.github.nest.arcteryx.meta.beans.internal.BeanPropertyDescriptor"
					p:name="code"
					p:description="Code of educationDegree" />
			</list>
		</property>
		<property name="operators">
			<list>
				<bean
					class="com.github.nest.sparrow.party.internal.operator.EducationDegreeCacheProvider"
					init-method="readFromClassPath"
					p:fileClassPath="/codes/educationDegrees.list" />
			</list>
		</property>
	</bean>
</beans>